[user]
	name = Brad Montgomery
    email = brad@bradmontgomery.net
[github]
    user = bradmontgomery
    token = REALTOKENHERE
[core]
    autocrlf = input
    safecrlf = true
    editor = vim
    excludesfile = /Users/brad/.gitignore_global
[alias]
    co = checkout
    ci = commit
    st = status
    stat = status
    br = branch 
    hist = log --pretty=format:\"%Cred%h %Creset%ad | %s%Cgreen%d %Cblue[%an]%Creset\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    dif = diff --color
[color]
    ui = true
    branch = true
    diff = true
    status = true
[color "branch"]
    current = yellow reverse
    local = yellow 
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
